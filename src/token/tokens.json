{
  "TokenStudio": {
    "primitive": {
      "color": {
        "base": {
          "white": { "$value": "#ffffff", "$type": "color" },
          "black": { "$value": "#18181b", "$type": "color" }
        },
        "neutral": {
          "100": { "$value": "#f8f8f8", "$type": "color" },
          "500": { "$value": "#b4b4b4", "$type": "color" },
          "700": { "$value": "#565656", "$type": "color" },
          "900": { "$value": "#2f2f2f", "$type": "color" }
        },
        "blue": {
          "500": { "$value": "#387adf", "$type": "color" }
        },
        "purple": {
          "500": { "$value": "#604fed", "$type": "color" },
          "600": { "$value": "#5746e2", "$type": "color" }
        },
        "red": {
          "500": { "$value": "#ff3757", "$type": "color" }
        }
      },
      "size-unit": {
        "0": { "$value": 0, "$type": "number" },
        "4": { "$value": 4, "$type": "number" },
        "8": { "$value": 8, "$type": "number" },
        "16": { "$value": 16, "$type": "number" },
        "24": { "$value": 24, "$type": "number" }
      }
    },
    "semantic": {
      "color": {
        "text": {
          "primary": { "$value": "{base.black}", "$type": "color" },
          "secondary": { "$value": "{neutral.700}", "$type": "color" },
          "inverse": { "$value": "{base.white}", "$type": "color" }
        },
        "bg": {
          "primary": { "$value": "{base.white}", "$type": "color" },
          "secondary": { "$value": "{neutral.100}", "$type": "color" },
          "brand": { "$value": "{purple.500}", "$type": "color" }
        },
        "border": {
          "primary": { "$value": "{neutral.500}", "$type": "color" }
        }
      },
      "size": {
        "space": {
          "0": { "$value": "{size-unit.0}", "$type": "dimension" },
          "4": { "$value": "{size-unit.4}", "$type": "dimension" },
          "8": { "$value": "{size-unit.8}", "$type": "dimension" },
          "16": { "$value": "{size-unit.16}", "$type": "dimension" },
          "24": { "$value": "{size-unit.24}", "$type": "dimension" }
        }
      }
    }
  }
}
